{
  "name": "front",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "type-check": "tsc",
    "type-check-dev": "tsc --watch",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier-check": "prettier --check \"*.js\" \"*.ts\" \"./src/*\"",
    "prettier-write": "prettier --write \"*.js\" \"*.ts\" \"./src/*\"",
    "watch": "webpack --mode development --watch",
    "build": "webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.21",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.6.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "antd": "^4.19.2",
    "axios": "^0.26.1",
    "core-js": "^3.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9"
  }
}
